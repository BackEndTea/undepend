{
    "name": "backentea/undepend",
    "description": "Detects orphaned dependencies in your composer.json",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gert de Pagter",
            "email": "BackEndTea@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Undepend\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Undepend\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5||^9.0",
        "infection/infection": "^0.15.3",
        "phpstan/phpstan": "^0.12.11",
        "vimeo/psalm": "^3.9",
        "doctrine/coding-standard": "^7.0"
    },
    "bin": ["bin/undepend"]
}
